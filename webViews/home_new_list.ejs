<%- partial('common/header',{title:title,cssSrc:[]}) %>
<%- partial('common/layout_header',{}) %>
<%- partial('common/menu',{}) %>
<!-- 页面内容 -->
<div class="container tab_container">
	<h6><%= JSON.stringify(brandDetailData) %></h6>
	<section id="hot_product"></section>
	<section id=""></section>
	<section id="new_list">
		<ul class="brand_list">
		<% brandDetailData.products.forEach(function(v){ %>
			<li>
				<a class="pic" href="javascript:;"><img src=<%= v.pic_url %> alt=""></a>
				<p class="title" data-value="<%= v.title %>"></p>
				<div class="price">
					<div class="discount">优选价:<%= v.cur_price%></div>
					<div class="original"><%= v.og_price%></div>
				</div>
				<div class="info">
					<div class="sales">已售<%= v.sales %>件</div>
					<div class="cart"><a href="javascript:;"></a></div>
				</div>
			</li>	    
		<% }) %>
		</ul>
	</section>
	<section id="brand_list"></section>
</div>

<%- partial('common/layout_footer',{}) %>
<%- partial('common/footer',{jsSrc:[]}) %>